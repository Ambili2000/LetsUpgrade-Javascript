
public class Linear search  {
   
    int a[]={10,20,30,40,50,30};  //5
   
    int search_ele=50;

    for(int i=0;i<a.length;i++)
    {
        //system.out.printIn(a[i]);

        if(search_ele==a[i])

        {
            system.out.printIn(element found at; "+1);
            break;
        }
    }

    if (fiag==false)
    {
        system.out.printIn(Element Not found");
    }
}        
